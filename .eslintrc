{
    "env": {
        "browser": true,
        "jasmine": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 5,
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],

        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": [
            "error", { 
                "allow": [
                    "warn", "error"
                    ] 
            }
        ]
    }
    
}