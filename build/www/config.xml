<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android" id="cat.neix.tags" version="3.8.0" versionCode="30800">
  <name>tagsPRO</name>
  <description>Gestión de la Comunicación para Entidades (PREPRODUCCION)</description>
  <author href="http://neix.cat" email="info@neix.cat">Neix Devolpment TEAM</author>
  <preference name="phonegap-version" value="cli-6.4.0"/>  
  <preference name="permissions" value="none"/>
  <preference name="orientation" value="default"/>
  <preference name="target-device" value="universal"/>
  <preference name="webviewbounce" value="true"/>
  <preference name="prerendered-icon" value="true"/>
  <preference name="stay-in-webview" value="false"/>
  <preference name="StatusBarOverlaysWebView" value="true" /> 
  <preference name="StatusBarBackgroundColor" value="#000000" />
  <preference name="StatusBarStyle" value="lightcontent" />
  <preference name="detect-data-types" value="true"/>
  <preference name="exit-on-suspend" value="false"/>
  <preference name="show-splash-screen-spinner" value="true"/>
  <preference name="auto-hide-splash-screen" value="true"/>
  <preference name="disable-cursor" value="false"/>
  <preference name="android-installLocation" value="auto"/>
  <preference name="android-build-tool" value="gradle" />
  <gap:plugin name="cordova-plugin-dialogs" spec="1.3.0" source="npm" />
  <gap:plugin name="cordova-plugin-geolocation" spec="2.4.0" source="npm" />
  <gap:plugin name="cordova-plugin-inappbrowser" spec="1.5.0" source="npm" />
  <gap:plugin name="cordova-plugin-statusbar" spec="2.2.0" source="npm" />  
  <gap:plugin name="cordova-plugin-network-information" spec="1.3.0" source="npm" />
  <gap:plugin name="cordova-plugin-file" spec="4.3.0" source="npm" />
  <gap:plugin name="cordova-plugin-device" spec="1.1.3" source="npm" />
  <gap:plugin name="phonegap-plugin-push" spec="~1.8.4" source="npm">
    <param name="SENDER_ID" value="365687509925" />
  </gap:plugin>  
  <gap:plugin name="cordova-plugin-whitelist" source="npm" spec="1.3.0"/>
  <gap:plugin name="uk.co.workingedge.phonegap.plugin.launchnavigator" source="npm" spec="2.6.0"/>
  <gap:plugin name="cordova-plugin-appinfo" source="npm" spec="2.0.2" />
  <gap:plugin name="cordova-plugin-app-preferences" source="npm" spec="0.99.2"/>
  <gap:plugin platform="android" name="cordova-plugin-config-alert" version="1.0.0" source="npm"/>
